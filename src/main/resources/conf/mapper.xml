<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="top.yukino.shpee.conf.Mapper">
    <select id="select" resultType="java.util.Map">
        <![CDATA[
	  		${value}
	  	]]>
    </select>
    <insert id="insert">
        <![CDATA[
	  		${value}
	  	]]>
    </insert>
    <delete id="delete">
        <![CDATA[
	  		${value}
	  	]]>
    </delete>
    <update id="update">
        <![CDATA[
	  		${value}
	  	]]>
    </update>

    <select id="selsctTUpload" parameterType="java.lang.String" resultType="top.yukino.shpee.bean.Upload">
        SELECT UID,FILE_NAME,HASH,PATH,TYPE,FILE_SIZE,UPTIME
        FROM T_UPLOAD
        WHERE ISDELETE=0 AND UID=#{uid}
    </select>
</mapper>